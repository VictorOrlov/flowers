    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.1.6",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.17.0",
    "react-app-polyfill": "^1.0.4",
    "require-from-string": "^2.0.2",
    "resolve-url-loader": "^3.1.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "source-map-support": "^0.5.12",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "waait": "^1.0.5",
    "webpack": "^4.31.0"
  },
