    "cross-env": "^5.2.0",
    "cross-fetch": "^3.1.5",
    "dotenv": "^8.0.0",
    "emotion": "^10.0.9",
    "formik": "^1.5.8",
    "graphql": "^14.3.0",
    "graphql-tag": "^2.10.1",
    "history": "^4.9.0",
    "kcors": "^2.2.2",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
